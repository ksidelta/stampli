{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "CommonJS",
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": false,
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "outDir": "build",
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]
    },
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
